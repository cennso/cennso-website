    "@types/react": "18.2.78",
    "@types/react-dom": "18.2.18",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "lighthouse": "^11.4.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
